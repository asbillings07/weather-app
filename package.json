{
  "name": "weather_coding_exercise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "moment": "^2.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "jest --reporters=jest-nyancat-reporter --watchAll",
    "preview": "vite preview",
    "_deploy": "netlify deploy --dir=build --prod",
    "_encrypt": "npx staticrypt build/index.html Warn3rM3d1a -o build/index.html",
    "publish": "npm run build && npm run _deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "standard": {
    "globals": [
      "test",
      "expect",
      "fetch",
      "jest",
      "beforeEach"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@vitejs/plugin-react": "^4.0.4",
    "@webpack-cli/generators": "^3.0.7",
    "jest-nyancat-reporter": "^2.0.0",
    "standard": "^14.3.1",
    "vite": "^4.4.9"
  }
}
